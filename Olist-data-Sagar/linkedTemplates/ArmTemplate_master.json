{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Olist-data-Sagar"
		},
		"ADFToADLSLinkedService_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADFToADLSLinkedService'"
		},
		"AzureDatabricksOlist_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricksOlist'"
		},
		"SQLDBToADFLinkedService_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SQLDBToADFLinkedService'"
		},
		"ADFToADLSLinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://olistdatastoragesagar.dfs.core.windows.net/"
		},
		"AzureDatabricksOlist_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0724-122818-ikr889zv"
		},
		"AzureSynapseAnalyticsbyPool_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "synapse-workspace-sagar.sql.azuresynapse.net"
		},
		"AzureSynapseAnalyticsbyPool_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "olistsynapsepool"
		},
		"GitHubToADFLinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/Sagar1302/"
		},
		"HttpServerForSQLSynapse_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/Sagar1302/BigDataProject/refs/heads/main/SQLSynapsePath.json"
		},
		"RelativeURLFromGitHubToADFLinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/Sagar1302/BigDataProject/refs/heads/main/data.json"
		},
		"SQLDBToADFLinkedService_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "ncqlax.h.filess.io"
		},
		"SQLDBToADFLinkedService_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "OlistDatabase_movieblock"
		},
		"SQLDBToADFLinkedService_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "OlistDatabase_movieblock"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Olist-data-Sagar_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADFToADLSLinkedService_accountKey": {
						"value": "[parameters('ADFToADLSLinkedService_accountKey')]"
					},
					"AzureDatabricksOlist_accessToken": {
						"value": "[parameters('AzureDatabricksOlist_accessToken')]"
					},
					"SQLDBToADFLinkedService_password": {
						"value": "[parameters('SQLDBToADFLinkedService_password')]"
					},
					"ADFToADLSLinkedService_properties_typeProperties_url": {
						"value": "[parameters('ADFToADLSLinkedService_properties_typeProperties_url')]"
					},
					"AzureDatabricksOlist_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricksOlist_properties_typeProperties_existingClusterId')]"
					},
					"AzureSynapseAnalyticsbyPool_properties_typeProperties_server": {
						"value": "[parameters('AzureSynapseAnalyticsbyPool_properties_typeProperties_server')]"
					},
					"AzureSynapseAnalyticsbyPool_properties_typeProperties_database": {
						"value": "[parameters('AzureSynapseAnalyticsbyPool_properties_typeProperties_database')]"
					},
					"GitHubToADFLinkedService_properties_typeProperties_url": {
						"value": "[parameters('GitHubToADFLinkedService_properties_typeProperties_url')]"
					},
					"HttpServerForSQLSynapse_properties_typeProperties_url": {
						"value": "[parameters('HttpServerForSQLSynapse_properties_typeProperties_url')]"
					},
					"RelativeURLFromGitHubToADFLinkedService_properties_typeProperties_url": {
						"value": "[parameters('RelativeURLFromGitHubToADFLinkedService_properties_typeProperties_url')]"
					},
					"SQLDBToADFLinkedService_properties_typeProperties_server": {
						"value": "[parameters('SQLDBToADFLinkedService_properties_typeProperties_server')]"
					},
					"SQLDBToADFLinkedService_properties_typeProperties_database": {
						"value": "[parameters('SQLDBToADFLinkedService_properties_typeProperties_database')]"
					},
					"SQLDBToADFLinkedService_properties_typeProperties_username": {
						"value": "[parameters('SQLDBToADFLinkedService_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}